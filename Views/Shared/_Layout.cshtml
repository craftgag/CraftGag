<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  Welcome to CraftGag</title>
    <link rel="SHORTCUT ICON" type="image/png" sizes="16x16" href="~/img/favicon.png">

    @Styles.Render("~/Content/css")
   
</head>
<body class="scroll-assist" data-reveal-selectors="section:not(.masonry):not(:first-of-type):not(.parallax)" data-reveal-timing="1000">
    <a id="top"></a>
    <div class="loader"></div>
    @{
        Html.RenderPartial("_header");
    }
   
        <div class="main-container transition--fade">
            @RenderBody()
            
            @{
                Html.RenderPartial("_footer");
            }
        </div>
      

    @Scripts.Render("~/bundles/jquery")   
  
</body>
</html>
